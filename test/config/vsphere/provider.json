{
    "url": "${GOVC_URL}",
    "uid": "${GOVC_USERNAME}",
    "password": "${GOVC_PASSWORD}",
    "insecure": true,
    "dc": "${GOVC_DATACENTER}",
    "datastore": "${GOVC_DATASTORE}",
    "resource-pool": "${GOVC_RESOURCE_POOL}",
    "vmFolder": "${GOVC_FOLDER}",
    "timeout": 300,
    "template-name": "${GOVC_TEMPLATE_NAME}",
    "template": false,
    "linked": false,
    "customization": "",
    "test-mode": ${TEST_MODE},
    "network": {
        "domain": "${PUBLIC_DOMAIN_NAME}",
        "dns": {
            "search": [
                "${PUBLIC_DOMAIN_NAME}"
            ],
            "nameserver": [
                "${DOMAIN_RESOLVER}"
            ]
        },
        "interfaces": [
            {
                "primary": true,
                "exists": true,
                "network": "${GOVC_NETWORK_PRIVATE}",
                "adapter": "vmxnet3",
                "mac-address": "generate",
                "nic": "eth0",
                "dhcp": false,
                "address": "192.168.2.26",
                "gateway": "192.168.2.254",
                "netmask": "255.255.255.0"
            },
            {
                "primary": false,
                "exists": true,
                "network": "${GOVC_NETWORK_PUBLIC}",
                "adapter": "vmxnet3",
                "mac-address": "generate",
                "nic": "eth1",
                "dhcp": true,
                "use-dhcp-routes": true
            }
        ]
    }
}
